<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.1.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_1_0.dtd">

<service-builder package-path="AlamatSvc">
	<namespace>master</namespace>
	<!--<entity data-source="sampleDataSource" local-service="true" name="Foo" remote-service="false" session-factory="sampleSessionFactory" table="foo" tx-manager="sampleTransactionManager uuid="true"">-->
	<entity local-service="true" name="Alamat" remote-service="true" uuid="true">

		<!-- PK fields -->

		<column name="id_alamat" primary="true" type="String" />

		<!-- Audit fields -->
		
		<column name="id_user" type="long" />
		<column name="nama_alamat" type="String" />
		<column name="nama_penerima" type="String" />
		<column name="nomor_handphone" type="long" />
		<column name="alamat_lengkap" type="String" />
		<column name="kode_pos" type="long" />
		<column name="kota" type="String" />
		<column name="provinsi" type="String" />
		<column name="region" type="String" />
		<column name="status" type="boolean" />
		<column name="created_date" type="Date" />
		<column name="modified_date" type="Date" />
		
	</entity>
</service-builder>